@using Umbraco.Cms.Core.Models;
@model BlogWebsite.Business.Features.Shared.Components.Navigation.NavigationViewModel

<nav-base home-url="@Model.Home?.Url()" home-name="@Model.Home?.Title">
    @foreach (var item in Model.Items)
    {
        if (item.Page is Link link && !string.IsNullOrEmpty(link.Url) && !string.IsNullOrEmpty(link.Name))
        {
            <nav-item name="@link.Name" url="@link.Url" target="@link.Target"></nav-item>
        }
    }
</nav-base> 