@using BlogWebsite.Data.Models.ModelsBuilder
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Home>
@{
    Layout = "Main.cshtml";
}

<div>HOMEPAGE</div>

@if (Model is IComponentsComposition componentsComposition)
{
    if (componentsComposition.Components != null && componentsComposition.Components.Any())
    {
        @await Html.PartialAsync("~/Views/Partials/Components.cshtml", componentsComposition.Components)
    }
}