{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|\\Umbraco.sdf;Flush Interval=1;"
  },
  "Umbraco": {
    "CMS": {
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "ModelsDirectory": "~/../BlogWebsite.Data/Models/ModelsBuilder",
        "ModelsNamespace": "BlogWebsite.Data.Models.ModelsBuilder"
      },
      "Hosting": {
        "Debug": false
      },
      "Global": {
        "Id": "4de1f744-378a-4e5a-a3ad-9d6343bb3946"
      }
    }
  }
}